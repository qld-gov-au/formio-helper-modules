          "components": [
            {
              "label": "Watermark",
              "tag": "div",
              "attrs": [
                {
                  "attr": "style",
                  "value": "position: fixed;bottom:0px;right:0px;opacity: 0.6;font-size: 30em;width: 100%;text-align: center;color: black;z-index: -1;"
